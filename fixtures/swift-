// Swift grammar has no rule for number separators.
0.0
1_000

// Swift grammar has no rule for commas in a tuple.
let a = (a, b)

// Swift grammar has no rule for colons and type names.
let a: Int?

// Swift grammar has no rule for the hash prefix and commas between arguments.
if #available(iOS 15.4.1, *) {}

// Swift grammar has no rule for commas in structure properties.
class C {
  var p: Int
}

// Swift grammar has no rule for dots in member access.
a.b

// Swift grammar has no rule for array brackets and commas between elements.
[0, 1]
